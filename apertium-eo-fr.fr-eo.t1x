	      <let>
	        <var n="nombre"/>
	        <clip pos="1" side="tl" part="nbr"/>
	      </let>
<let>
<var n="xxx"/>
<lit-tag v="3b"/>
</let>
	    </otherwise>
	  </choose>
	</when>

	<otherwise> <!-- 4: aliokaze, ni elektas la nombron de la unua elemento (eblus de la dua: io ajn) -->
