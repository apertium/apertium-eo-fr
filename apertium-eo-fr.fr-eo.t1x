	<let><var n="nprep"/><clip pos="1" side="tl" part="lem"/></let>

	<out>
	  <chunk namefrom="nprep" case="caseFirstWord">
	    <tags>
	      <tag><lit-tag v="pr"/></tag>
	    </tags>
	    <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="tags"/>
	    </lu>
	  </chunk>
	</out>	
	<let>
	  <var n="caseFirstWord"/>
	  <lit v="aa"/>
	</let>
      </action>
    </rule>



<!--******** REGLES DE SN ******************************************** -->

