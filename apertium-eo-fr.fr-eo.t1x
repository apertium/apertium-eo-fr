         	<clip pos="1" side="sl" part="lem"/>
         	<list n="qui"/> <!-- per a pronoms  -->
      	   </in>
	</or>
    	</test>
      	<let>
		<var n="changecase"/>
		<lit-tag v="sense"/>
      	</let>
  	</when>

  	<when>
    	<test>
      	   <in>
         	<clip pos="1" side="sl" part="lem"/>
         	<list n="quin"/> <!-- per a determinats interrogatius (van davant del verb) -->
      	   </in>
    	</test>
