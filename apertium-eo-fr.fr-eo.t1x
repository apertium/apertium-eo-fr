    <def-macro n="tipus_verbs" npar="1"> <!-- tots els verbs passen per aqui -->
      <choose>
        <when>
          <test>
          <in>
         <clip pos="1" side="sl" part="lemh"/>
         <list n="plaire"/>
          </in>
          </test>
          <let>
            <var n="tipus_verb"/>
            <lit-tag v="like"/>
          </let>
        </when>

	<when>
    	<test>
     	 <in>
       	  <clip pos="1" side="sl" part="lemh"/>
       	  <list n="moviment"/>
      	</in>
    	</test>
 	<let>
          <var n="tipus_verb"/>
          <lit-tag v="moviment"/>
        </let>
        </when>

    <when>
    <test>
      <in>
         <clip pos="1" side="sl" part="lemh"/>
         <list n="volition"/>
      </in>
    </test>
    <let>
     <var n="tipus_verb"/>
     <lit-tag v="volition"/>
