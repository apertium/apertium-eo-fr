	        <equal>
		  <clip pos="1" side="sl" part="a_num"/>
		  <lit-tag v="num.ciph"/>
	        </equal>
	      </test>
<let>
<var n="xxx"/>
<lit-tag v="4b"/>
</let>
	      <choose>
