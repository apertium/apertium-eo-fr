  	</when>
  	<otherwise>
      	<let>
		<var n="changecase"/>
		<lit-tag v="nom"/>
      	</let>
  	</otherwise>
	</choose>
      	<let>
		<var n="case"/>
		<lit-tag v="nom"/>
      	</let>
    </def-macro>

