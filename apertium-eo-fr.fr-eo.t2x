	<pattern-item n="SN"/>
	<pattern-item n="SNobj"/>
	<pattern-item n="SVlex"/>
	<pattern-item n="SNacc"/>
	<pattern-item n="cnjcoo"/>
	<pattern-item n="SNacc"/>
      </pattern>
      <action>

	    <let>
	      <clip pos="4" part="case"/>
	      <lit-tag v="acc"/>
	    </let>
	    <let>
	      <clip pos="6" part="case"/>
	      <lit-tag v="acc"/>
	    </let>
	    <let>
	      <clip pos="2" part="tipus_prn"/>
	      <lit-tag v="subj"/>
	    </let>

	<out>
	  <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
	    <clip pos="1" part="chcontent"/>
	  </chunk>
	  <b pos="1"/>

	  <chunk>
	    <get-case-from pos="2">
	      <lit v="al"/>
	    </get-case-from>
	    <lit-tag v="pr"/>
	    <lit v="{^"/>
	    <get-case-from pos="2">
	      <lit v="al"/>
	    </get-case-from>
	    <lit-tag v="pr"/>
	    <lit v="$}"/>
	  </chunk>
	  <b/>
	  <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
	    <clip pos="2" part="chcontent"/>
	  </chunk>
	  <b pos="1"/>
	  <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
	    <clip pos="3" part="chcontent"/>
	  </chunk>
	  <b pos="2"/>
	  <chunk>
            <clip pos="4" part="lem"/>
            <clip pos="4" part="tags"/>
	    <clip pos="4" part="chcontent"/>
	  </chunk>
	  <b pos="3"/>
 	      <chunk>
                <clip pos="5" part="whole"/>
              </chunk>
	  <b pos="4"/>
	  <chunk>
            <clip pos="6" part="lem"/>
            <clip pos="6" part="tags"/>
	    <clip pos="6" part="chcontent"/>
	  </chunk>
	</out>
      </action>
</rule>
 
