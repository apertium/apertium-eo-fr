                <lit-tag v="prn.subj"/>
                <clip pos="3" part="pers"/>
                <lit-tag v="GD"/>
                <clip pos="3" part="nbr"/>
                <lit v="{^"/>
                <get-case-from pos="3">
                  <lit v="prpers"/>
                </get-case-from>
                <lit-tag v="prn.subj.3.4.5"/>
                <lit v="$}"/>
              </chunk>
              <b/>

	  <b pos="3"/>
